    <Authors>Notificacao Api</Authors>
    <Product>API Notificacao Api</Product>
    <AssemblyName>NotificacaoPubSub.Api</AssemblyName>
    <Company>Notificacao Api</Company>
    <Copyright>Notificacao Api</Copyright>
    <UserSecretsId>a0be56a1-53d8-498d-9375-7757f9f12520</UserSecretsId>
